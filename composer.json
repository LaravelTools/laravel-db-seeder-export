{
    "name": "laravel-toolkit/laravel-db-seeder-export",
    "description": "Export Laravel database tables to seeders with Telegram and email backup functionality",
    "keywords": ["laravel", "database", "seeder", "export", "backup"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelToolkit\\DbSeederExport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelToolkit\\DbSeederExport\\DatabaseSeederExportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}